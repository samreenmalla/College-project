<% @posts.each do |post| %>
	<%= image_tag post.user.photo.url.to_s %>
  <%= post.user.name %>
	<%= image_tag post.photo.url.to_s %>
  <%= post.caption %>
  <% if post.user == current_user %>
  <%= link_to 'Edit Post', edit_post_path(post.id) %>
  <%= link_to 'Delete Post', post_path(post.id), method: :destroy %>
  <% end %>
<% end %>

